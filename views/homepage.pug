doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body
        h1 Custom Object Table
        a(href="/update-cobj") Add/Update this table
        br
        br     
        table(style='width:40%', border='1')
            tr
                th Name
                th Email
                th Student Grade
            each student in data
                tr
                    td #{student.properties.name}
                    td #{student.properties.email}
                    td #{student.properties.student_grade}