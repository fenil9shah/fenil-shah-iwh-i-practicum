doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body
        h1 Custom Object Table
        table(style='width:50%', border='1', align='center')
            tr
                th Name
                th Email
                th Student Grade
            each student in data
                tr
                    td #{student.properties.name}
                    td #{student.properties.email}
                    td #{student.properties.student_grade}